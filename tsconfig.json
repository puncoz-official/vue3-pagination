{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "nodenext",
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "allowSyntheticDefaultImports": true,
        "strictFunctionTypes": false,
        "jsx": "preserve",
        "baseUrl": ".",
        "allowJs": false,
        "sourceMap": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "importHelpers": true,
        "noImplicitAny": true,
        "skipLibCheck": false,
        "lib": ["esnext", "dom"],
        "types": ["vite/client"],
        "noEmit": false,
        "outDir": "dist",
        "declaration": true,
        "declarationDir": "dist",
        "paths": {
            "@/*": ["Pagination/*"],
            "#/*": ["Pagination/@types/*"]
        }
    },
    "include": ["env.d.ts", "Pagination/**/*.ts", "Pagination/**/*.d.ts", "Pagination/**/*.vue"],
    "exclude": ["node_modules"]
}
